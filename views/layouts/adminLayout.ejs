<script>
  let activeDatePicker=null;
  /****************************************
   *       Basic Table                   *
   ****************************************/


  $(document).ready(function(){

    $('.carousel').carousel({
      interval: false
    });
    /*$(function () {
      $('[data-toggle="popover"]').popover()
    })*/


    $(".preloader").fadeOut();

    $('[data-toggle="tooltip"]').tooltip()

    //***********************************//
    // For select 2
    //***********************************//
    $(".select2").select2({
      tags: true,
      tokenSeparators: [',', ' '],


    });

    let width =$(window).width()
    if(width >= 768 && width <= 1024 ){
      $('#main-wrapper').addClass('mini-sidebar')
      $('#main-wrapper').attr('data-sidebartype','mini-sidebar')
    }
    //***********************************//
    // Datepicker
    //***********************************//
    $('.form_datetime').datetimepicker({

      //language:  'fr',
      // weekStart: 1,
     // todayBtn:  1,
      //autoclose: 1,
     // todayHighlight: 1,
      //startView: 2,
      //forceParse: 0,
      //showMeridian: 1,
      pickerPosition: "bottom-left",
      startDate: new Date(),
      format:"yyyy-mm-dd"



    }).on('show',function () {
      activeDatePicker=this;
    })
  })
</script>
<script>

  $(document).on('mouseenter','.hoverable-custom',function() {
    $(this).find('.hidden-custom').css('display', 'block')
    $('[data-toggle="popover"]').popover()
  })
  $(document).on('mouseleave','.hoverable-custom',function() {
    $(this).find('.hidden-custom').css('display', 'none')
    $('[data-toggle="popover"]').popover()
  })
</script>

