  <script>
    var loadFromControllerAndToComponent = function (url,data, successCallBack, errorCallBack,method = 'GET') {
      $.ajax({
        url: url,
        method: method,
        data: data,
        success: function (responseData) {
          successCallBack && successCallBack(responseData)
        },
        failure: function (msg) {
          errorCallBack && errorCallBack(msg)
        }
      });
    };
